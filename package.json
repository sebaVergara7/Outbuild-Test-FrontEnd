{
  "name": "outbuild-test-front-end",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{json,css,scss,md}": "prettier"
  },
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean:cache": "npm cache clean --force",
    "clean:modules": "npx rimraf node_modules",
    "clean:dist": "npx rimraf dist",
    "clean": "npm run clean:cache && npm run clean:modules && npm run clean:dist",
    "reinstall": "npm run clean && npm install",
    "prepare": "husky install",
    "server": "ts-node server.ts",
    "prebuild": "npm run lint && npm run type-check",
    "postbuild": "echo 'Build completed!'",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "framer-motion": "^12.4.11",
    "next": "15.2.1",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/next": "^8.0.7",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.12",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
